{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "src/server.js" }],
  "rewrites": [
    { "source": "/login", "destination": "/src/server.js" },
    { "source": "/register", "destination": "/src/server.js" },
    { "source": "/(.*)", "destination": "/src/server.js" }
  ]
}
